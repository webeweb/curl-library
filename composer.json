{
    "name": "webeweb/curl-library",
    "type": "library",
    "description": "Integrate cURL with your projects.",
    "keywords": [
        "curl",
        "library",
        "webeweb"
    ],
    "homepage": "https://github.com/webeweb/curl-library",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "webeweb/traits-library": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\Curl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\Curl\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/curl-library/issues"
    }
}
